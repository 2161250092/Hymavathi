sno=int(input('Enter student number'))
sname=input('Enter student name')
group=input('Enter group')
s1=int(input('Enter maths marks'))
s2=int(input('Enter stat marks'))
s3=int(input('Enter computer marks'))
total=s1+s2+s3
avg=total/3
print('sno',sno)
print('sname',sname)
print('group',group)
print('Result:')
if s1>35:
    if s2>35:
        if s3>35:
            print('pass')
else:
    print('fail')
